#!/home/timothy/projects/hug_website_runner/venv/bin/python3.6
# EASY-INSTALL-ENTRY-SCRIPT: 'hug==2.2.0','console_scripts','hug'
__requires__ = 'hug==2.2.0'
import re
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.argv[0] = re.sub(r'(-script\.pyw?|\.exe)?$', '', sys.argv[0])
    sys.exit(
        load_entry_point('hug==2.2.0', 'console_scripts', 'hug')()
    )
